# vim: ft=cfg
[default]
baudrate=115200
timestamp=1

[Xiphos Q7]
pattern=q7-rev[ab]?-([a-f0-9]{4})
tty=/dev/serial/by-id/usb-Xiphos_Systems_Corp._Q7-PIM_%s-if00-port0

